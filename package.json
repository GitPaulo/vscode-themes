{
  "name": "gitpaulo-vscode-themes",
  "displayName": "GitPaulo VSCode Themes",
  "description": "@GitPaulo's VSCode Themes!",
  "version": "0.0.7",
  "publisher": "GitPaulo",
  "scripts": {
    "build": "node scripts/build-themes.js",
    "dev": "npm run build && rm -f gitpaulo-vscode-themes-*.vsix && vsce package && npm run extension",
    "extension": "code --install-extension gitpaulo-vscode-themes-*.vsix"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "GitPaulo Github",
        "uiTheme": "vs-dark",
        "path": "./themes/gitpaulo_github.json"
      },
      {
        "label": "GitPaulo Tsoding",
        "uiTheme": "vs-dark",
        "path": "./themes/gitpaulo_tsoding.json"
      },
      {
        "label": "GitPaulo Original",
        "uiTheme": "vs-dark",
        "path": "./themes/gitpaulo_original.json"
      },
      {
        "label": "GitPaulo Minimal",
        "uiTheme": "vs-dark",
        "path": "./themes/gitpaulo_minimal.json"
      },
      {
        "label": "GitPaulo Gruvbox",
        "uiTheme": "vs-dark",
        "path": "./themes/gitpaulo_gruvbox.json"
      }
    ]
  }
}
