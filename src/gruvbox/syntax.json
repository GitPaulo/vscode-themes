{
  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7c6f64"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a9b665"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.nil",
        "constant.language.none",
        "constant.language.null.go",
        "variable.language.null",
        "variable.language.nil",
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false",
        "meta.property-name",
        "meta.property-value",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#7daea3"
      }
    },
    {
      "scope": [
        "support.function",
        "support.constant.builtin"
      ],
      "settings": {
        "foreground": "#e78a4e"
      }
    },
    {
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#d3869b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "support.type.primitive",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.numeric.go",
        "storage.type.cs",
        "keyword.type.null.ts"
      ],
      "settings": {
        "foreground": "#d8a657"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "meta.property.object",
        "meta.property-name",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#d4be98"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#EAE2B9"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#478aff"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#a866d8"
      }
    },
    {
      "scope": [
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "punctuation.other.placeholder",
        "string.interpolated",
        "meta.interpolation",
        "constant.other.placeholder.format",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.interpolation.begin.csharp",
        "punctuation.definition.interpolation.end.csharp",
        "punctuation.definition.interpolation.begin.python",
        "punctuation.definition.interpolation.end.python",
        "string.interpolated",
        "meta.interpolation"
      ],
      "settings": {
        "foreground": "#EAE2B9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#EAE2B9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "unnecessary",
      "settings": {
        "foreground": "#ff6464",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "parameter": {
      "foreground": "#d4be98"
    },
    "type": {
      "foreground": "#e78a4e"
    },
    "namespace": {
      "foreground": "#7daea3"
    },
    "macro": {
      "foreground": "#e78a4e"
    }
  },
  "_syntaxDescription": "Code highlighting rules for all programming languages"
}
