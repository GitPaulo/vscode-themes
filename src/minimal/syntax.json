{
  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "comment",
        "string.quoted.docstring",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#cdc168"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.character.escape",
        "constant.regexp",
        "keyword.operator.quantifier.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#D8D8A0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.other",
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.nil",
        "constant.language.none",
        "constant.language.null.go",
        "variable.language.null",
        "variable.language.nil"
      ],
      "settings": {
        "foreground": "#8BB4D8"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.preprocessor",
        "entity.name.tag",
        "entity.name.section",
        "entity.name.label",
        "entity.name.goto-label",
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.struct",
        "entity.name.namespace",
        "entity.name.type.namespace",
        "entity.name.type.module",
        "entity.name.module",
        "entity.name.package",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "entity.other.attribute",
        "keyword",
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.new",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.typeof",
        "keyword.operator.expression.instanceof",
        "keyword.other.unit",
        "keyword.control.directive",
        "keyword.control.import.include",
        "keyword.type.null.ts",
        "storage.modifier",
        "storage.type",
        "storage.type.annotation",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.numeric.go",
        "storage.type.cs",
        "support.type",
        "support.type.primitive",
        "support.type.builtin",
        "support.type.sys-types",
        "support.type.object.module",
        "support.class",
        "support.function",
        "support.constant.builtin",
        "support.constant.handlebars",
        "support.variable.property",
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.property",
        "variable.parameter",
        "variable.language",
        "variable.language.special",
        "variable.language.arguments",
        "punctuation.definition.parameters",
        "punctuation.definition.bracket",
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.annotation",
        "punctuation.section.parens",
        "punctuation.section.brackets",
        "punctuation.accessor",
        "punctuation.separator.pointer-access",
        "meta.property-name",
        "meta.property-value",
        "meta.property.object",
        "meta.function-call.generic",
        "meta.function.parameters",
        "meta.function.parameter",
        "meta.parameters",
        "meta.method.declaration",
        "meta.method",
        "meta.brace.round",
        "meta.brace.square",
        "meta.preprocessor",
        "meta.preprocessor.macro",
        "meta.import",
        "meta.require",
        "meta.tag",
        "meta.tag.inline",
        "meta.tag.block"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "scope": [
        "variable.language.this",
        "variable.language.super",
        "variable.other.constant",
        "variable.other.enummember",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "punctuation.decorator",
        "storage.modifier.async",
        "keyword.control.async",
        "keyword.control.await"
      ],
      "settings": {
        "foreground": "#ffffffde",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "punctuation.definition.interpolation.begin.csharp",
        "punctuation.definition.interpolation.end.csharp",
        "punctuation.definition.interpolation.begin.python",
        "punctuation.definition.interpolation.end.python",
        "punctuation.definition.template-expression",
        "punctuation.other.placeholder",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "string.interpolated",
        "meta.interpolation",
        "constant.other.placeholder.format"
      ],
      "settings": {
        "foreground": "#ffffffde"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#478aff"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#a866d8"
      }
    },
    {
      "scope": "unnecessary",
      "settings": {
        "foreground": "#ff6464",
        "fontStyle": "underline"
      }
    }
  ],
  "_syntaxDescription": "Minimal color scheme: white text with subtle grey accents"
}
