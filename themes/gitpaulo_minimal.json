  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6A6A6A"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#cdc168"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.character.escape",
        "constant.regexp",
        "keyword.operator.quantifier.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#D8D8A0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function.console"
      ],
      "settings": {
        "foreground": "#EBEBEB"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "variable.parameter",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "entity.name.type.namespace",
        "entity.name.type.module",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#ECECEC"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#B8D0D0"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.nil",
        "constant.language.none",
        "constant.language.null.go",
        "variable.language.null",
        "variable.language.nil",
        "meta.property-name",
        "meta.property-value",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "punctuation.definition.parameters",
        "punctuation.section.parens",
        "punctuation.section.brackets",
        "punctuation.definition.bracket",
        "meta.brace.round",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#A8BEBE"
      }
    },
    {
      "scope": [
        "support.function",
        "support.constant.builtin"
      ],
      "settings": {
        "foreground": "#B8B8B8"
      }
    },
    {
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "support.type.primitive",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.numeric.go",
        "storage.type.cs",
        "keyword.type.null.ts"
      ],
      "settings": {
        "foreground": "#B8B8B8"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "meta.property.object",
        "meta.property-name",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#A8A8A8"
      }
    },
    {
      "scope": [
        "meta.function.parameters",
        "meta.function.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#B8B8B8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.label",
        "entity.name.goto-label"
      ],
      "settings": {
        "foreground": "#D0D0D0"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#B8D0D0"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute"
      ],
      "settings": {
        "foreground": "#C8C8C8"
      }
    },

    {
      "scope": [
        "variable.other.constant",
        "variable.other.enummember",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "support.type",
        "support.type.builtin",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#C8C8C8"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "keyword.control.directive",
        "keyword.control.import.include",
        "meta.preprocessor.macro"
      ],
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": [
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#B8B8B8"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": [
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.enum",
        "entity.name.type.struct"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "string.quoted.docstring",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "storage.type.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#B8B8B8"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.language.special",
        "variable.language.arguments"
      ],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "scope": [
        "meta.import",
        "meta.require"
      ],
      "settings": {
        "foreground": "#D8D8D8"
      }
    },
    {
      "scope": [
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#E0E0E0"
      }
    },
    {
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": [
        "meta.method.declaration",
        "meta.method"
      ],
      "settings": {
        "foreground": "#EBEBEB"
      }
    },
    {
      "scope": [
        "storage.modifier.async",
        "keyword.control.async",
        "keyword.control.await"
      ],
      "settings": {
        "foreground": "#B8B8B8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.typeof",
        "keyword.operator.expression.instanceof"
      ],
      "settings": {
        "foreground": "#B8B8B8"
      }
    },
    {
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.pointer-access"
      ],
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": [
        "meta.tag",
        "meta.tag.inline",
        "meta.tag.block"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#478aff"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#a866d8"
      }
    },
    {
      "scope": [
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "punctuation.other.placeholder",
        "string.interpolated",
        "meta.interpolation",
        "constant.other.placeholder.format",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.interpolation.begin.csharp",
        "punctuation.definition.interpolation.end.csharp",
        "punctuation.definition.interpolation.begin.python",
        "punctuation.definition.interpolation.end.python"
      ],
      "settings": {
        "foreground": "#B0B0B0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#B0B0B0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "unnecessary",
      "settings": {
        "foreground": "#ff6464",
        "fontStyle": "underline"
      }
    }
  ],
  "_syntaxDescription": "Minimal color scheme: white text with subtle grey accents"
}